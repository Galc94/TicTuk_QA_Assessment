{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "printWidth": 120,
  "quoteProps": "as-needed",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "overrides": [
    {
      "files": [
        "*.json"
      ],
      "options": {
        "trailingComma": "none",
        "singleAttributePerLine": true,
        "bracketSameLine": false
      }
    }
  ]
}
